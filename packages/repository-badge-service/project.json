{
  "name": "repository-badge-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/repository-badge-service/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/repository-badge-service",
        "main": "packages/repository-badge-service/src/index.ts",
        "tsConfig": "packages/repository-badge-service/tsconfig.lib.json",
        "assets": ["packages/repository-badge-service/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/repository-badge-service/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/repository-badge-service/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
